Algoritmo "semnome"

Var
   n1, n2: inteiro

Inicio
   escreva("Digite o primeiro número: ")
   leia(n1)
   escreva("Digite o segundo número: ")
   leia(n2)
   se(n1 < n2) entao
      escreva("O número",n2," é maior que",n1,"!")
   senao
      escreva("O número",n1," é maior que",n2,"!")
   fimse

Fimalgoritmo