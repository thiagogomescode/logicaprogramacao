Algoritmo "semnome"

Var
   valor_real, valor_dolar, dolares: real


Inicio
   escreva("Dê o valor em reais: ")
   leia(valor_real)
   escreva("Dê a cotação do dólar: ")
   leia(valor_dolar)
   dolares <- (valor_real / valor_dolar)
   escreva("O valor  convertido para dólar é:",dolares," dólares!!!")






Fimalgoritmo